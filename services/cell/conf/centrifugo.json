{
    "api_disable": true,
    "allowed_origins": [
        "*"
    ],
    "allow_anonymous_connect_without_token": true,
    "proxy_publish": false,
    "proxy_subscribe": true,
    "allow_subscribe_for_client": true,
    "grpc_api": true,
    "grpc_api_address": "0.0.0.0",
    "grpc_api_port": 10000,
    "proxy_subscribe_endpoint": "grpc://backend:10001",
    "proxy_subscribe_timeout": "20s",
    "proxy_rpc_endpoint": "grpc://backend:10001",
    "proxy_rpc_timeout": "20s",
    "namespaces": [{
        "name": "sheets",
        "proxy_subscribe": true
    }]
}
